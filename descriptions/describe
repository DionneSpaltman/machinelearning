#%%
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
#%%
# describing the distribution of the year
x = pd.read_json('/Users/fabianwaldmann/machinelearning/machinelearning/machinelearning/train.json')
print(x.describe())
print(x.columns)
x_year = x["year"]
print(x_year)
#%%
print(x.shape)
print(x.info())
print(x.head())
# %%
plt.hist(x_year, bins=50, density=True, alpha=0.7, color='blue', edgecolor='black')

# Add labels and a title
plt.xlabel('Year')
plt.ylabel('Frequency')
plt.title('Distribution Plot')

# Show the plot
plt.show()

