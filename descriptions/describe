#%%
import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
#%%
# describing the distribution of the year
x = pd.read_json('/Users/fabianwaldmann/machinelearning/machinelearning/machinelearning/train.json')
x.describe()
#%%
print(x.shape)
print(x.info())
print(x.head())
# %%
plt.hist(x, bins=30, density=True, alpha=0.7, color='blue', edgecolor='black')

# Add labels and a title
plt.xlabel('Value')
plt.ylabel('Frequency')
plt.title('Distribution Plot')

# Show the plot
plt.show()

